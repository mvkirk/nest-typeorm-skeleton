{
  "type": "mysql",
  "host": "localhost",
  "port": 3306,
  "username": "DATABASE_USER_NAME",
  "password": "DATABASE_PASSWORD",
  "database": "YOUR_DATABASE",
  "entities": ["dist/**/*.entity{.ts,.js}"],
  "synchronize": true,
  "migrationsTableName": "custom_migration_table",
  "migrations": ["db/migrations/*.js"],
  "cli": {
      "migrationsDir": "db/migration",
      "entitiesDir": "src"
  }
}